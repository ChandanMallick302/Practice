package com.insertTable;


public class cxcx {

	public static void main(String[] args) {
		
		int id[]= {1,2,3};
		String name[]= {"Chandan","Rajesh","Ramesh"};
		for(int i=0;i<id.length;i++) {
			
			for(int j=0;j<i;j++) {
				System.out.print(id[i]);
				System.out.println(name[j]);
			}
		}
		System.out.println();
	}

}
