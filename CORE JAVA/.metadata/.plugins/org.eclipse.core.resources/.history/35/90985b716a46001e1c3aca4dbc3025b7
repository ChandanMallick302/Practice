package HashMap;

import java.util.HashMap;
import java.util.Map;

public class HashMap_Map {

	public static void main(String[] args) {
		Map<Integer, String> hm=new HashMap<>();
		
		//Adding Key and objects
		hm.put(1, "Apple");
		hm.put(4, "Orange");
		hm.put(2, "Mango");
		hm.put(1, "Grapes");
		
		System.out.println(hm);
		
		//HasMap is the child of Map
		//it is preserved key order
		//it does not allow duplicate
	}

}
