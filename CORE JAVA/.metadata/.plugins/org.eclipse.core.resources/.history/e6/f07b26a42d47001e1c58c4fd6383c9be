package file_Operation;

import java.io.File;

public class Fetching_File_Information {

	public static void main(String[] args) {
		File file=new File("src/file_storage/LC.txt");
		
		try {
			if (file.exists()) {
				System.out.println();
			} else {
				System.out.println();
			} 
		} catch (Exception e) {
			// TODO: handle exception
		}
	}

}
