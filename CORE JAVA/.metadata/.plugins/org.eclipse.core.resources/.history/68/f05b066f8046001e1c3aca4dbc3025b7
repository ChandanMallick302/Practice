package Type_Safe;

public class TypeSafe<T> {
	T=container;
	private Object value;
	
	
	public static void main(String[] args) {
		
		TypeSafe<String> t1=new TypeSafe<>();
		t1.setValue("dada");
		t1.setId(1);
		System.out.println(t1.getId()+" "+t1.getValue());
		
		
		TypeSafe<String> t2=new TypeSafe<>();
		t2.setValue(455);
		t2.setId(2);
		System.out.println(t2.getId()+" "+t2.getValue());

		
		//Here no type is mention so you can use any type of data
		
		

	}

}
