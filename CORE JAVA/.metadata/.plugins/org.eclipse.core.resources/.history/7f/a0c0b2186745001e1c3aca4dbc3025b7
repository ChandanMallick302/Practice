package Local_inner_class;

public class LocalInnerClass {
	private int data = 30;

	void display() {
		class Local {
			void msg() {
				System.out.println(data);
			}
		}
		Local l1 = new Local();
		l1.msg();
	}

	public static void main(String[] args) {
		LocalInnerClass l1 = new LocalInnerClass();
		l1.display();
	}
}
