package com.practice.constructor;

class a{
	//Non-Parameterized Constructor
	public a() {
		System.out.println("Non-Parameterized Constructor");
	}
	// Parameterized Constructor
	public a(int i,String c){
		System.out.println("Parameterized Constructor");
	}
	// overloading constructor
	public a(String c,int i){
		super();
		System.out.println("overloading constructor 1");
	}
	// overloading constructor by narrowing method
	public a(short i,String c) {
		System.out.println("overloading constructor 2");
	}
	public a(int i) {
		System.out.println("overloading constructor 3");
	}
	public a(String c) {
	System.out.println("overloading constructor");
	}
}
public class Constructors {


	public static void main(String[] args) {
		

	}

}
