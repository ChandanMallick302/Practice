package Find_Minimum_value_of_an_Array;

public class MinimumArrayValue {

	public static void printArray(int arr[]) {
		for(int i=0; i<arr.length;i++) {
			int j = arr[i];
		}
		System.out.println();
	}
	public static void reverseArray(int arr[]) {
		//set the first position of array to min variable
		int min=arr[0];
		for(int i=0;i<arr.length;i++) {
			//min is greater than other position of array then store that value in min  
			if(min>arr[i]) {
				min=arr[i];
			}
		}
		return min;
	}
	public static void main(String[] args) {
		int arr[]= {5,9,3,5,1,2};
		System.out.print("Find the minimum value from ");
		printArray(arr);
		System.out.println("The minimum value is ");
	}
}