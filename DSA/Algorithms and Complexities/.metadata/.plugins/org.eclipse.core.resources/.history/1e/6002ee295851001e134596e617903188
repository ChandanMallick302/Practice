package Adding_or_Updating_elements_in_a_One_Dimensional_Array;

public class ArrayUtil {

	public void printArray(int[]arr) {
		int n=arr.length;
		for(int i=0;i<n;i++) {
			System.out.print(arr[i]+" ");
		}
		System.out.println();
	}
	
	public void arrayDemo() {
		int[] myArray=new int[5];
		myArray[0]=5;
		myArray[1]=1;
		myArray[2]=8;
		myArray[3]=2;
		myArray[4]=10;
		myArray[4]=10;
		myArray[4]=10;
		printArray(myArray);
	}
	public static void main(String[] args) {
		ArrayUtil a1=new ArrayUtil();
		a1.arrayDemo();

	}

}
