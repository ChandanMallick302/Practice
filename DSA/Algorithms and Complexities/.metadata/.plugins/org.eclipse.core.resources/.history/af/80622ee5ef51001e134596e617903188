		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i]+" ");
		}
		System.out.println();
		
	}
	public int findMissingNumber(int arr[]) {
		return null;
	}
	public static void main(String[] args) {
		
	}

}
